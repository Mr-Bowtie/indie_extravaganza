{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^7.3.0",
    "@smoosee/vite-plugin-sass": "^1.0.3",
    "bulma": "^0.9.4",
    "esbuild": "^0.18.20",
    "sass": "^1.65.1",
    "tslib": "^2.6.2"
  },
  "scripts": {
    "build": "esbuild $(find app/frontend/javascript -name '*.js') --bundle --sourcemap --outdir=app/assets/builds --public-path=/assets",
    "build:css": "sass ./app/frontend/stylesheets/application.scss:./app/assets/stylesheets/application.css --no-source-map --load-path=node_modules"
  },
  "packageManager": "yarn@1.22.19",
  "devDependencies": {
    "vite": "^4.3.0",
    "vite-plugin-full-reload": "^1.0.0",
    "vite-plugin-ruby": "^3.2.0",
    "vite-plugin-stimulus-hmr": "^3.0.0"
  }
}
